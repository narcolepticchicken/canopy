# Developer EOA used to issue capabilities (secp256k1 private key in hex)
ISSUER_ECDSA_PRIVATE_KEY=

# Server port to listen on (default 8787)
PORT=8787

# Chain ID for the EAS instance
EAS_CHAIN_ID=

# Address of the EAS contract
EAS_ADDRESS=

# Schema UID used for EAS attestations
EAS_SCHEMA_UID=

# Path to an OPA-compiled Wasm policy file
POLICY_WASM_PATH=
